{
  "name": "fluxr",
  "version": "0.5.0",
  "description": "An application template for applications using a Node + Express + React + MongoDB stack with Flux architecture.",
  "author": "Anthony Dodd",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/TheDodd/fluxr",
  "repository": {
    "type": "git",
    "url": "https://github.com/TheDodd/fluxr"
  },
  "bugs": {
    "url": "https://github.com/TheDodd/fluxr/issues"
  },
  "keywords": [
    "Flux",
    "React"
  ],
  "main": "src/js/main.js",
  "scripts": {
    "build": "gulp default",
    "buildpkg": "slc build --install --scripts --bundle --pack",
    "start": "gulp default && nodemon server/server.js",
    "test": "jsxhint app server && jest",
    "watch": "gulp watch"
  },
  "dependencies": {
    "body-parser": "1.9.3",
    "cors": "2.5.2",
    "express": "4.10.2",
    "flux": "2.0.1",
    "mongoose": "3.8.x",
    "object-assign": "1.0.0",
    "objectid": "3.1.0",
    "react": "0.12.0"
  },
  "devDependencies": {
    "browserify": "^6.3.3",
    "del": "^1.0.0",
    "gulp": "^3.8.10",
    "gulp-streamify": "^0.0.5",
    "gulp-uglify": "^1.0.1",
    "jest-cli": "^0.1.18",
    "jsxhint": "^0.7.0",
    "nodemon": "^1.2.1",
    "react-tools": "^0.12.1",
    "reactify": "^0.17.1",
    "vinyl-source-stream": "^1.0.0"
  },
  "jest": {
    "scriptPreprocessor": "./preprocessor.js",
    "unmockedModulePathPatterns": [
      "./node_modules/react"
    ]
  },
  "bundleDependencies": [
    "body-parser",
    "cors",
    "express",
    "flux",
    "mongoose",
    "object-assign",
    "objectid",
    "react"
  ]
}
